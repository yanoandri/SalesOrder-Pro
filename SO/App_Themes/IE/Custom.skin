<%-- CompareValidator --%>
<asp:CompareValidator runat="server" CssClass="Validation" Display="Dynamic" ForeColor="Red" SkinID="FilterList" Font-Size="11px" />


<%-- RadPanelBar --%>
<telerik:RadPanelBar runat="server" ExpandMode="FullExpandedItem" Width="310px" Height="310px" SkinID="RadPanelBarDashboard" CssClass="RadPanelBarDashboard"> </telerik:RadPanelBar>

<telerik:RadDatePicker 
    runat="server" 
    Skin="Default"
    CssClass="TextBox"
    DateInput-DisplayDateFormat="MM/dd/yyyy"
    DateInput-DateFormat="MM/dd/yyyy" 
    Culture="en"
    AllowEmpty="True"
    radcontrolsdir="~/RadControls/" 
    MinDate="1900-01-01"
    SkinID="FilterList"
    Width="135px">
</telerik:RadDatePicker>

